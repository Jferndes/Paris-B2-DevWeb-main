{% extends "base.html.twig" %} {% block css %}
<link rel="stylesheet" href="/css/list.css" />
{% endblock %} {% block body %} {% include "navbar.html.twig" %} {% if msg %}
<div class="alert alert-{{ msg[1] }}" role="alert">
  {{ msg[0] }}
</div>
{% endif %}

<div class="container">
  <table class="table">
    <div class="card">
      <thead>
        <tr>
          <th>Id</th>
          <th>Email</th>
        </tr>
      </thead>
      <tbody>
        {% for user in users %}
        <tr>
          <td>{{ user.id }}</td>
          <td>{{ user.email }}</td>
        </tr>
        {% endfor %}
      </tbody>
    </div>
  </table>
</div>
{% endblock body %}
